{
  "name": "prettier-standard-vscode",
  "displayName": "Prettier-Standard - JavaScript formatter",
  "description": "VS Code plugin for prettier + standard",
  "version": "0.6.0",
  "publisher": "numso",
  "author": "Dallin Osmun <@dallinosmun>",
  "galleryBanner": {
    "color": "#ffe04b",
    "theme": "light"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=numso.prettier-standard-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/numso/prettier-standard-vscode.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/numso/prettier-standard-vscode/issues"
  },
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onLanguage:json5",
    "onLanguage:typescript",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less",
    "onLanguage:graphql",
    "onLanguage:markdown",
    "onLanguage:mdx",
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:yaml",
    "onLanguage:mdx",
    "onLanguage:flow",
    "onLanguage:angular"
  ],
  "main": "./src/extension",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "prettier-standard": "16.1.0"
  }
}
